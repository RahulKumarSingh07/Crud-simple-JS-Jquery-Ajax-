
@{
    ViewData["Title"] = "Index";
}

<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.4/css/jquery.dataTables.css">

<div id="popup"></div>
<div class="container">
    <button class="btn btn-primary" data-url="@Url.Action("Create")" data-toggle="modal" data-target="#Add User">Add User</button><br /><br />
    <div class="table-responsive">
        <table id="userdetails" class="table table-striped table-bordered ">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Gender</th>
                    <th>Mobile No</th>
                    <th>Email_Id</th>
                    <th>Address</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>

</div>
<!-- Modal -->



@section Scripts{
    <script src="~/js/JavaScript.js"></script>
}
